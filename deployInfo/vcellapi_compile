#!/bin/bash

#
# include common variable definitions
#
DIR=${0%/*}
if [[ ! -d "$DIR" ]]; then DIR="$PWD"; fi
. $DIR/vcell.include
. $DIR/vcellapi.include

#
# make sure no variables are left unset
#
shopt -s -o nounset

classpath="${vcellapi_jars}:${common_jvm_jars}"

javac -cp ${classpath} -d ${vcellapi_root}/bin -sourcepath ${vcellapi_root}/src ${vcellapi_root}/src/org/vcell/rest/VCellApiMain.java


